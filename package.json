{
  "name": "request-kit-client",
  "version": "0.8.3",
  "description": "Reusable authentication and API client",
  "main": "dist/request-kit-client.umd.js",
  "module": "dist/request-kit-client.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/request-kit-client.es.js",
      "require": "./dist/request-kit-client.umd.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run types",
    "types": "tsc --project tsconfig.build.json",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "api-client",
    "axios",
    "auth",
    "typescript",
    "token",
    "sdk",
    "http",
    "react"
  ],
  "author": "Mohammed Syed Awadh",
  "license": "MIT",
  "peerDependencies": {
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "axios": "^1.10.0",
    "c8": "^10.1.3",
    "rollup-plugin-dts": "^6.2.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vitest": "^3.2.4"
  }
}
